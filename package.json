{
  "name": "r2l-api",
  "version": "1.0.0",
  "description": "An API for Rights2LIFE",
  "main": "main.js",
  "scripts": {
    "build": "npx tsc && npx tsc-alias",
    "start": "node dist/main.js",
    "dev": "ts-node-dev --env-file=.env -r tsconfig-paths/register src/main.ts",
    "test": "npx jest",
    "introspect": "drizzle-kit introspect:pg --config=drizzle.config.ts",
    "generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
    "migrate": "tsx -r env $(cat .env) src/database/migrate.ts",
    "studio": "drizzle-kit studio"
  },
  "keywords": [
    "Rights2LIFE"
  ],
  "author": "Adrian Mallari",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.30.2",
    "drizzle-zod": "^0.5.1",
    "express": "^4.18.3",
    "express-session": "^1.18.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "tsc-alias": "^1.8.8"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.26",
    "@types/pg": "^8.11.2",
    "drizzle-kit": "^0.20.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.1"
  }
}
